                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="rollback"> 
                                <xsd:annotation> 
                                    <xsd:documentation>Limpiar la conexión mediante retrotracción.</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="enableConnectionCasting" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
