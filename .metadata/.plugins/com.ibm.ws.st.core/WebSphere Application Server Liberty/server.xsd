                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="6"> 
                                <xsd:annotation> 
                                    <xsd:documentation>6</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="7"> 
                                <xsd:annotation> 
                                    <xsd:documentation>7</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
